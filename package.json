{
  "name": "dutrinkst",
  "version": "1.0.0",
  "description": "justKile and Astrobra presents: DuTrinkst",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf target",
    "test": "npm run unit",
    "server": "gulp serve",
    "watch": "npm run clean && (npm run server & gulp watch & npm run unit:watch)",
    "start": "npm run clean && npm run browser:build:dev && npm run server",
    "deploy": "npm run build && (npm run browser:deploy & npm run server:deploy)",
    "server:deploy": "gulp deploy:page",
    "browser:deploy": "gulp deploy:assets",
    "browser:build": "gulp build:client && gulp html",
    "browser:build:dev": "gulp build:client && gulp html:dev",
    "unit:server": "echo 'No server tests'",
    "unit:browser": "ava lib/browser/**/*.spec.js --source 'lib/browser/**/*!(spec).js' --require ./test/browser/setup-browser-env.js --require babel-register --require ignore-styles",
    "unit:server:watch": "npm run unit:server -- --watch",
    "unit:browser:watch": "npm run unit:browser -- --watch",
    "unit": "npm run unit:server && npm run unit:browser",
    "unit:watch": "npm run unit:server:watch & npm run unit:browser:watch",
    "build": "npm run clean && npm run browser:build && npm run lint && npm run unit",
    "lint": "eslint --ext .js --ext .jsx lib/",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "Ben Lenser",
  "license": "MIT",
  "dependencies": {
    "confidence": "^3.0.2",
    "lodash": "^4.16.4",
    "preact": "^8.2.7",
    "preact-compat": "^3.17.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "ava-spec": "^1.0.1",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-register": "^6.16.3",
    "bootstrap-styl": "^5.0.8",
    "browser-env": "^2.0.12",
    "browser-sync": "^2.17.2",
    "css-loader": "^0.25.0",
    "d3": "^4.2.8",
    "enzyme": "^2.5.1",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "gulp": "^3.9.1",
    "gulp-ava": "^0.14.1",
    "gulp-awspublish": "^3.3.0",
    "gulp-compile-handlebars": "^0.6.1",
    "gulp-rename": "^1.2.2",
    "gulp-serve": "^1.4.0",
    "ignore-styles": "^5.0.1",
    "jeet": "^6.1.5",
    "js-yaml": "^3.10.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.16.4",
    "mkdirp": "^0.5.1",
    "react-addons-test-utils": "^15.3.2",
    "require-dir": "^0.3.1",
    "run-sequence": "^2.2.0",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack-node-externals": "^1.5.4",
    "webpack-stream": "^3.2.0"
  },
  "ava": {
    "concurrency": 5,
    "failFast": true,
    "tap": false,
    "powerAssert": false,
    "babel": "inherit",
    "verbose": true
  }
}
